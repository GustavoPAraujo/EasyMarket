
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Store {
  id          Int           @id @default(autoincrement())
  name        String
  description String?
  adminId     Int           @unique // ou n√£o, dependendo do relacionamento desejado
  createdAt   DateTime      @default(now())
}

